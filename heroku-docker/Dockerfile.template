FROM $DOCKER_NAMESPACE/$DOCKER_NOMBRE_IMAGEN:$DOCKER_TAG
CMD gunicorn \
    -b ${DOLAR}PYTHON_HOST:${DOLAR}PORT \
    --reload \
    --workers=${DOLAR}PYTHON_GUNICORN_WORKERS \
    --worker-connections=${DOLAR}PYTHON_GUNICORN_CONNECTIONS \
    ${DOLAR}PYTHON_NOMBRE_APP:${DOLAR}PYTHON_NOMBRE_FUNCION_APP